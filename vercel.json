

{
  "version": 2,
  "builds": [
    { "src": "api/generate.js", "use": "@vercel/node" },
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/generate", "dest": "api/generate.js" },
    { "src": "/", "dest": "index.js" },
    { "src": "/(.*)", "dest": "index.js" }
  ]
}
